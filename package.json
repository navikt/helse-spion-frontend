  "name": "frontend-template",
  "version": "0.1.0",
  "private": true,
  "homepage": "/min-side-refusjon",
  "scripts": {
    "start": "env-cmd -f .env.local react-app-rewired start",
    "build:local": "npm run lint && env-cmd -f .env.local react-app-rewired start",
    "build:dev": "npm run lint && env-cmd -f .env.dev react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "install": "react-app-rewired build",
    "test": "react-app-rewired test --color --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.ts*'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "eslintConfig": {
    "extends": "react-app"
